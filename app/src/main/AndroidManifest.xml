<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eventmanagment">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".service_update_info"></activity>
        <activity android:name=".service_update" />
        <activity android:name=".service_delete" />
        <activity android:name=".service_add" />
        <activity android:name=".booking_view" />
        <activity android:name=".location_search_info" />
        <activity android:name=".location_delete" />
        <activity android:name=".location_delete_show" />
        <activity android:name=".location_view" />
        <activity android:name=".location_update" />
        <activity android:name=".register_user" />
        <activity android:name=".owner" />
        <activity android:name=".location_update_show" />
        <activity android:name=".search_location" />
        <activity android:name=".add_location" />
        <activity android:name=".home" />
        <activity android:name=".user" />
        <activity android:name=".admin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>